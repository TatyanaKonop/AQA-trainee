version: '3'
services:
    pytets:
        build . 
        container_name: pytets
    chrome:
        image: selenium/node-chrome:4.12.1-20230912
        shm_size: 2gb
        
        depends_on:
        - selenium-hub
        environment:
        - SE_EVENT_BUS_HOST=selenium-hub
        - SE_EVENT_BUS_PUBLISH_PORT=4442
        - SE_EVENT_BUS_SUBSCRIBE_PORT=4443


